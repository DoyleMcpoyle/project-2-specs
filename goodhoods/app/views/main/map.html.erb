<h1>Welcome <%= @found_user.username %>!</h1> <%= link_to "Logout", logout_path, method: :delete %>

<h1>THE MAP</h1>

<%= form_tag search_path, method: "get", id:"search-input", class:"controls" do %>
  <%= label_tag(:city, "City: ") %>
  <%= text_field_tag(:city) %>
  <%= label_tag(:state, "State: ") %>
  <%= text_field_tag(:state) %>
  <%= label_tag(:neighborhood, "Neighborhood: ") %>
  <%= text_field_tag(:neighborhood) %>
  <%= submit_tag("Search") %>
<% end %>

<div id="map-canvas"></div>


<div id="city-summary"></div>

<div id="people"></div>

<div id="characteristics"></div>

<div id="ages"></div>

<div id="kids"></div>

<div id="relationships"></div>

<div id="charts"></div>



